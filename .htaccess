<IfModule mod_rewrite.c>
 RewriteEngine On
RewriteRule ^taskManager/php/api/(.+)/(.+)/(.+)$ taskManager/php/api.php?cmd=$1&subcmd=$2&param=$3 [L]
 RewriteRule ^taskManager/php/api/(.+)/(.+)$ taskManager/php/api.php?cmd=$1&subcmd=$2 [L]
 RewriteRule ^taskManager/php/api/(.+)$ taskManager/php/api.php?cmd=$1 [L]
 </IfModule>
